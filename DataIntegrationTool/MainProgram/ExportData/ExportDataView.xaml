<UserControl x:Class="DataIntegrationTool.MainProgram.ExportData.ExportDataView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             DataContext="{Binding ExportData,
                                   Source={StaticResource Locator}}"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <StackPanel>
        <Button Command="{Binding ButtonPushedCommand}"
                Content="Press Me" />
        <TextBlock Text="{Binding ButtonValue}" />
    </StackPanel>
</UserControl>
